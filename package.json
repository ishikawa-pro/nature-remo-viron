{
  "name": "my_viron",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "dependencies": {
    "@types/config": "0.0.36",
    "axios": "^0.20.0",
    "config": "^3.3.2",
    "dayjs": "^1.9.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "googleapis": "^60.0.1",
    "jsonwebtoken": "^8.5.1",
    "nature-remo-api-client-ts": "^1.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-express-middleware": "^4.0.2",
    "typeorm": "^0.2.28",
    "viron-page-definer": "^0.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "@types/swagger-express-middleware": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "dev": "PORT=3000 ts-node-dev --ignore-watch node_modules --inspect=53000 -- src/index.ts",
    "build": "npm run clear && npm run format && npm run lint && tsc && npm run generate:swagger-json",
    "clear": "rm -rf dist",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '{src,__tests__}/**/*.{ts,tsx}'",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"{src,__tests__}/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint --fix \"{src,__tests__}/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "generate:swagger-json": "js-yaml swagger/swagger.yaml > swagger/swagger.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "__tests__/**/*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "files": [
    "dist",
    "config",
    "swagger"
  ],
  "author": "",
  "license": "ISC"
}
